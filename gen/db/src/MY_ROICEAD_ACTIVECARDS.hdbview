VIEW MY_ROICEAD_ACTIVECARDS ("COUNTOFACTIVECARDS" , "TYPENAME")
AS

select * from MY_ROICEAD_TOPACTIVECARDS
UNION ALL 
(select  count(TYPENAME) as COUNTOFACTIVECARDS, 'Others' AS TYPENAME
FROM "MY_ROICEAD_CARDS" where status = 'Active' and TYPENAME not in 
(SELECT	"TYPENAME"
FROM MY_ROICEAD_TOPACTIVECARDS ) group by TYPENAME )
