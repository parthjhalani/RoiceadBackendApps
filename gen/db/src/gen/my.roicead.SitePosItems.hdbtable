COLUMN TABLE my_roicead_SitePosItems (
  SiteTrnID NVARCHAR(10) NOT NULL,
  SiteTrnItem NVARCHAR(10) NOT NULL,
  MATERIAL_MaterialId NVARCHAR(18),
  QUANTITIY NVARCHAR(10),
  UOM NVARCHAR(5),
  UNIT_PRICE NVARCHAR(10),
  AMOUNT DECIMAL(15, 2),
  CURRENCY NVARCHAR(5),
  VAT_PERC NVARCHAR(10),
  PRIMARY KEY(SiteTrnID, SiteTrnItem)
) WITH ASSOCIATIONS (
  MANY TO ONE JOIN my_roicead_Materials AS MATERIAL ON (MATERIAL.MaterialId = MATERIAL_MaterialId)
)