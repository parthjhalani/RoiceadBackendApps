VIEW my_roicead_POSBYPRIMNW AS SELECT
  POSANALYTICS_0.PrimaryNetwork,
  POSANALYTICS_0.YearOfPay,
  sum(POSANALYTICS_0.Amount) AS Amount,
  POSANALYTICS_0.TOTAMOUNT,
  POSANALYTICS_0.YEARTODATEAMOUNT
FROM my_roicead_POSANALYTICS AS POSANALYTICS_0
GROUP BY POSANALYTICS_0.PrimaryNetwork, POSANALYTICS_0.TOTAMOUNT, POSANALYTICS_0.YearOfPay, POSANALYTICS_0.YEARTODATEAMOUNT