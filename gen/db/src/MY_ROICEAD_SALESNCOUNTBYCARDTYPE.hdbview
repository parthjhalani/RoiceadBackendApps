view MY_ROICEAD_SALESNCOUNTBYCARDTYPE
AS

select CARDS.TYPENAME, CARDS.GROSSVALUE, CARDS.VALUECURRENCY, COUNTOFCARDS.COUNTOFACTIVECARDS,COUNTOFTRANS."CountOfTransactions" AS COUNTOFTRANS  from
"MY_ROICEAD_ACTIVECARDS" AS COUNTOFCARDS INNER JOIN "MY_ROICEAD_NOOFTRANSBYCARDTYPE" AS COUNTOFTRANS
ON COUNTOFCARDS.TYPENAME = COUNTOFTRANS.TYPENAME
INNER JOIN "MY_ROICEAD_C_CARDS" AS CARDS ON CARDS.TYPENAME = COUNTOFCARDS.TYPENAME